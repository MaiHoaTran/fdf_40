<%= provide :title, t(".title") %>
<ol class="breadcrumb">
  <li class="breadcrumb-item active"><%= link_to t(".title"), admin_users_path %></li>
  <li class="breadcrumb-item">
    <%= link_to t(".add"), new_admin_user_path %>
  </li>
</ol>
<div class="card mb-3">
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th><%= t ".id" %></th>
            <th><%= t ".email" %></th>
            <th><%= t ".full_name" %></th>
            <th><%= t ".sex" %></th>
            <th><%= t ".phone" %></th>
            <th><%= t ".role" %></th>
            <th class="function"><%= t "admin.categories.index.function" %></th>
          </tr>
        </thead>
        <tbody>
          <%= render @users %>
        </tbody>
      </table>
      <%= will_paginate %>
    </div>
  </div>
</div>
